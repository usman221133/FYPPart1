    @model IEnumerable<KnowledgeHub.Models.GeneratedClassesByEF.Presenter_Table>
@{
    Layout = null;
}

<link href="~/Content/bootstrap.css" rel="stylesheet" />
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h1>Welcome to Teacher Portal</h1> <br />
    @*<div>
            @Html.ActionLink("Profile", "ViewProfile")
        </div>*@

    <table class="table table-bordered table-hover">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Presenter_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Presenter_City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Presenter_Email)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.Presenter_Ph_Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Presenter_Gender)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.DOB_Days)
            </th>

            <th>Action</th>
        </tr>

        @foreach (var item in Model)
        {
            string DOB = item.DOB_Days + "-" + item.DOB_Months + "-" + item.DOB_Year;
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Presenter_Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Presenter_City)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Presenter_Email)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Presenter_Ph_Num)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Presenter_Gender)
                </td>


                <td>

                    @Html.DisplayFor(modelItem => DOB)
                </td>

                <td>
                    @Html.ActionLink("Profile", "ViewProfile", new { id = item.Presenter_ID })

                </td>
            </tr>
        }

    </table>


</body>
</html>